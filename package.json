{
  "name": "stylable.io",
  "version": "1.0.0",
  "description": "The Stylable website https://stylable.io/",
  "private": true,
  "main": "N/A",
  "scripts": {
    "copy-assets": "ts-node build-assets.ts",
    "build:examples": "node node_modules/rabbit-playground/bin/src/cli/cli.js build-project-files --srcDir playgrounds --outDir generated-playgrounds",
    "build:watch-examples": "npm run build:examples -- --w",
    "deploy": "npm run copy-assets && npm run build:examples && bundle install && bundle exec jekyll build && gh-pages -d ./_site",
    "dev": "npm run copy-assets && run-p build:watch-examples jekyll:watch",
    "jekyll:watch": "bundle exec jekyll serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wixplosives/stylable.io.git"
  },
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/wixplosives/stylable.io/issues"
  },
  "homepage": "https://stylable.io/",
  "devDependencies": {
    "@types/node": "^9.4.0",
    "gh-pages": "^1.1.0",
    "kissfs": "^0.5.3",
    "npm-run-all": "^4.1.2",
    "prismjs": "^1.11.0",
    "rabbit-playground": "0.8.0",
    "stylable-playground-recepies": "^0.25.0",
    "tippy.js": "^2.1.1",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1"
  }
}
